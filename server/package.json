  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "migration:generate": "npx typeorm-ts-node-esm migration:generate ./src/migrations/new-migration -d ./src/data-source",
